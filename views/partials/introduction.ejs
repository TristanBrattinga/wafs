<section id="about" class="container introduction">
    <article id="dataContainer">
        <p><%= data.description %></p>
        <p>Some of my hobbies are:</p>
        <ul>
            <% data.hobbies.map(hobby => { %>
                <li>
                    <%= hobby %>
                </li>
            <% }) %>
        </ul>
    </article>
    <section>
        <ul>
            <li><img src="/images/me.jpg" alt="" /></li>
            <li><img src="/images/me2.jpg" alt="" /></li>
        </ul>
    </section>
</section>